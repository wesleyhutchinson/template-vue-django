{
  "name": "Base Vue Django Template",
  "description": "Bootstrap for Vue Django starter projects",
  "repository": "https://github.com/wesleyhutchinson/base-vue-django-template",
  "keywords": ["django", "vue", "vuejs", "template", "django rest framework"],
  "env": {
